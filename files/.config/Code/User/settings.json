{
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.accessibilitySupport": "off",
    "editor.cursorStyle": "block",
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
    "editor.insertSpaces": false,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "boundary",
    "editor.wrappingIndent": "indent",
    "extensions.ignoreRecommendations": true,
    "telemetry.telemetryLevel": "off",
    "window.autoDetectHighContrast": false,
    "workbench.colorTheme": "Solarized Dark",
    "workbench.commandPalette.history": 20,
    "workbench.enableExperiments": false,
    "workbench.preferredDarkColorTheme": "Solarized Dark",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none",
    "github.gitAuthentication": false,
    "shellcheck.enableQuickFix": false,
    "shellcheck.run": "onSave",
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.confirmOnExit": "hasChildProcesses",
    "terminal.integrated.confirmOnKill": "always",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.enableBell": true,
    "terminal.integrated.enablePersistentSessions": false,
    "breadcrumbs.enabled": false,
    "redhat.telemetry.enabled": false,
    "window.enableMenuBarMnemonics": false,
    "cSpell.userWords": [
        "ansible",
        "doas",
        "Grafana",
        "Guglielmo",
        "interruptible",
        "kube",
        "kubeconfig",
        "kubectl",
        "kubelet",
        "Kubernetes",
        "minikube",
        "shellcheck"
    ],
    "editor.find.cursorMoveOnType": false,
    "editor.smoothScrolling": true,
    "workbench.editor.sharedViewState": true,
    "workbench.editor.mouseBackForwardToNavigate": false,
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "cSpell.caseSensitive": true,
    "cSpell.minWordLength": 2,
    "cSpell.numSuggestions": 4,
    "cSpell.spellCheckDelayMs": 500,
    "cSpell.useGitignore": false,
    "vim.incsearch": false,
    "vim.surround": false,
    "files.insertFinalNewline": true,
    "python.experiments.enabled": false,
    "python.linting.mypyEnabled": true,
    "python.linting.flake8Enabled": true,
    "python.terminal.activateEnvironment": false,
    "python.languageServer": "Pylance",
    "vim.useSystemClipboard": true,
    "editor.copyWithSyntaxHighlighting": false,
    "explorer.confirmUndo": "verbose",
    "explorer.incrementalNaming": "smart",
    "explorer.openEditors.sortOrder": "fullPath",
    "explorer.openEditors.visible": 100
}
